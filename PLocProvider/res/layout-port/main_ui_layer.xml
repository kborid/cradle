<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ca_home_008" >
    <ImageView 
        android:layout_marginLeft="20dip"
        android:layout_marginTop="10dip"
        android:src="@drawable/ca_home_010"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <View
        android:id="@+id/center"
        android:layout_width="1px"
        android:layout_height="1px"
        android:layout_centerInParent="true" />
    
    <com.pioneer.cradle.PLocProvider.components.CircleView
        android:id="@+id/setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/center"
        android:layout_alignTop="@id/center"
        android:layout_marginRight="33dp"
        android:layout_marginTop="-110dp"
        android:src="@drawable/main_setting" />

    <com.pioneer.cradle.PLocProvider.components.CircleView
        android:id="@+id/navi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/center"
        android:layout_alignLeft="@id/center"
        android:layout_marginLeft="-77dp"
        android:layout_marginRight="12dp"
        android:src="@drawable/main_navi" />

    <com.pioneer.cradle.PLocProvider.components.CircleView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/center"
        android:layout_alignTop="@id/center"
        android:layout_marginLeft="-112dp"
        android:layout_marginTop="-10dp"
        android:src="@drawable/main_phone" />

    <com.pioneer.cradle.PLocProvider.components.CircleView
        android:id="@+id/music"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/center"
        android:layout_alignTop="@id/center"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="-54dp"
        android:src="@drawable/main_music" />

    

</RelativeLayout>
